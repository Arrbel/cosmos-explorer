# 环境配置示例文件
# 复制此文件为 .env.local 并填入实际值

# 应用环境
NODE_ENV=production

# API 配置
REACT_APP_API_URL=https://api.cosmos-explorer.com
REACT_APP_CDN_URL=https://cdn.cosmos-explorer.com

# 阿里云配置
ALIYUN_ACCESS_KEY_ID=your_access_key_id
ALIYUN_ACCESS_KEY_SECRET=your_access_key_secret
ALIYUN_OSS_BUCKET=cosmos-explorer-assets
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com

# Docker 配置
DOCKER_REGISTRY=registry.cn-hangzhou.aliyuncs.com
DOCKER_NAMESPACE=your-namespace
ALIYUN_DOCKER_USERNAME=your_docker_username
ALIYUN_DOCKER_PASSWORD=your_docker_password

# 服务器配置
PROD_HOST=your.production.server.com
PROD_USERNAME=root
DEV_HOST=your.development.server.com
DEV_USERNAME=root

# 监控配置
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxx/xxx/xxx

# 数据库配置（如果需要）
DATABASE_URL=postgresql://user:password@localhost:5432/cosmos_explorer

# Redis 配置（如果需要）
REDIS_URL=redis://localhost:6379
